<%- include('partials/header') -%>

<h1>Time To Shop!!</h1>

<ul class="all-items">
    <% for(var i=0; i<products.length; i++) {%>
      <li class="list-items">
        <a href="/product/<%= products[i]._id%>" class="each-item">
          <h2 class="photo-of-item">photo</h2>
          <p class="name-of-item"><%= products[i].name %></p>
          <p class="des-of-item"><%= products[i].description %></p>
        </a>
      </li>
    <% } %>
</ul>